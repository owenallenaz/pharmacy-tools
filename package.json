{
  "name": "pharmacy-tools",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "5.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "5.2.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "docker_build": "docker build -t pharmacy:local .",
    "docker_run": "docker run -p 3000:3000 -v $PWD/lib:/app/lib -v $PWD/src:/app/src -v $PWD/package.json:/app/package.json -v $PWD/tsconfig.json:/app/tsconfig.json -v $PWD/package-lock.json:/app/package-lock.json -it pharmacy:local bash",
    "docker": "npm run docker_build && npm run docker_run",
    "start": "CHOKIDAR_USEPOLLING=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
